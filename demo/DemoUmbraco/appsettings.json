{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Data Source=umbraco.db;Mode=ReadWrite;Cache=Shared",
    "umbracoDbDSN_ProviderName": "Microsoft.Data.Sqlite"
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "Id": "d3a84645-46af-4303-8247-63669af8b586"
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "DeliveryApi": {
        "Enabled": true,
        "PublicAccess": true,
        "OutputExpansion": {
          "Enabled": true
        }
      },
      "Unattended": {
        "InstallUnattended": true,
        "UnattendedUserName": "Admin",
        "UnattendedUserEmail": "admin@example.com",
        "UnattendedUserPassword": "Admin12345!"
      },
      "Security": {
        "AllowConcurrentLogins": false
      }
    }
  },
  "HeadlessPreview": {
    "BaseUrl": "http://localhost:3000",
    "PreviewSecret": "demo-preview-secret-12345",
    "PreviewEndpoint": "/api/preview",
    "PreviewLabel": "Preview on Frontend",
    "UseLocalization": true,
    "Enabled": true
  }
}

